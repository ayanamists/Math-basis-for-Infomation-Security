    (define (permutations s)

    \ \ (cond [(empty? s) empty]

    \ \ \ \ \ \ \ \ [(empty? (rest s)) (list s)]

    \ \ \ \ \ \ \ \ [else

    \ \ \ \ \ \ \ \ \ (let splice [(l '()) (m (first s)) (r (rest s))]

    \ \ \ \ \ \ \ \ \ \ \ (append

    \ \ \ \ \ \ \ \ \ \ \ \ (map (lambda (x) (cons m x))

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ (permutations (append l r)))

    \ \ \ \ \ \ \ \ \ \ \ \ (if (empty? r)

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ empty

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ (splice (cons m l) (car r) (cdr r)))))]))

    \;

    (define (vector-permutations vec)

    \ \ (map (\<lambda\> (x) (list-\<gtr\>vector x))

    \ \ \ \ \ \ \ (permutations (vector-\<gtr\>list vec))))

    \;

    (struct circle (list order)

    \ \ #:inspector #f)

    \;

    (define (simply-\<gtr\>normal cir)

    \ \ (let* ([vec (make-vector (circle-order cir) -1)]

    \ \ \ \ \ \ \ \ \ [atom-\<gtr\>normal

    \ \ \ \ \ \ \ \ \ \ (\<lambda\> (x)

    \ \ \ \ \ \ \ \ \ \ \ \ (let loop ([i 0])


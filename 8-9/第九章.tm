    \ \ \ \ \ \ \ \ (cons (normal-\<gtr\>atom vec (vector-ref vec i) i)

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ (search (+ i 1)))

    \ \ \ \ \ \ \ \ '()))

    \ \ (circle

    \ \ \ (reverse

    \ \ \ \ (foldl (\<lambda\> (x res) (if (or (null? x) (= (length x) 1))

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ res

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ (cons x res))) '()
    (search 0)))

    \ \ \ (circle-order cir)))

    \;

    (define (vector-merge! x y)

    \ \ (let loop ([i 0])

    \ \ \ \ (if (\<less\> i (vector-length x))

    \ \ \ \ \ \ \ \ (begin


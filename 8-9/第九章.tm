    \ \ \ \ \ \ \ \ [normal-y (simply-\<gtr\>normal cir-y)])

    \ \ \ \ (vector-merge! (circle-list normal-x)

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ (circle-list normal-y))

    \ \ \ \ (normal-\<gtr\>simply normal-x)))

    \;

    (define (circle^n cir n)


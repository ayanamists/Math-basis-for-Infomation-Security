    \ \ (let loop ([i 0])

    \ \ \ \ (if (\<gtr\> (vector-length x) i)

    \ \ \ \ \ \ \ \ (if (= (vector-ref x i)

    \ \ \ \ \ \ \ \ \ \ \ \ \ \ \ (vector-ref y i))

    \ \ \ \ \ \ \ \ \ \ \ \ (loop (+ i 1))

    \ \ \ \ \ \ \ \ \ \ \ \ #f)

    \ \ \ \ \ \ \ \ #t)))

    \;

    (define (circle-equal? cir-x cir-y)

